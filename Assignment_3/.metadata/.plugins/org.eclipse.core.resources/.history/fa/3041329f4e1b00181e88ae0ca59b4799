package model;

public class Sensor {
	private int rowr;
	private int colr;
	private RobotGrid robot;

	public Sensor(RobotGrid robot){
		this.robot = robot;
	}
	
	public void update(RobotGrid robot){
		this.robot = robot;
	}
	
	public int getRow(){
		return this.rowr;
	}
	
	public int getCol(){
		return this.colr;
	}
}
